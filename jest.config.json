{
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [ "src/metrics" ],
    "coverageReporters": ["json-summary", "text"],
    "verbose": true,
    "coverageThreshold": {
        "global": {
            "branches": 80,
            "statements": 80,
            "functions": 80,
            "lines": 80
        }
    }
}