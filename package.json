{
  "name": "jwt-pizza-service",
  "description": "Backend service for making JWT pizzas",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cd src && node index.js",
    "test": "jest",
    "lint": "eslint",
    "db:start": "source .env && docker run --name jwt-pizza-mysql -e MYSQL_ROOT_PASSWORD=$JWT_PIZZA_MYSQL_PASSWORD -p 3306:3306 -d -v ./.mysql/data:/var/lib/mysql -d mysql",
    "db:stop": "docker kill jwt-pizza-mysql"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/jest": "^29.5.13",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
